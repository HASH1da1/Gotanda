{
    "manifest_version": 2,
    "name": "Gotanda",
    "description": "OSINT addon",
    "version": "2.0.0",
    "icons": {
        "128": "image/128.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "clipboardWrite",
        "contextMenus",
        "tabs",
        "background"
    ],
    "browser_action":{
        "default_icon":{
            "19": "image/19.png",
            "38": "image/38.png"
        },
        "default_title": "Gotanda"
    }
}