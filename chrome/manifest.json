{
    "manifest_version": 2,
    "name": "Gotanda",
    "description": "Gotanda is Web Extension for OSINT.",
    "version": "0.1",
    "icons": {
    "128": "icons/128.png"
    },
    "permissions":[
        "clipboardWrite",
        "contextMenus",
        "background",
        "tabs"
    ],
    "background":{
        "scripts":["background.js"],
        "persistent": false
    }
}