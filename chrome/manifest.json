{
    "manifest_version": 2,
    "name": "Gotanda",
    "description": "Web Extension for OSINT.",
    "version": "0.1",
    "icons": {
        "128": "icons/128.png"
    },
    "background":{
        "scripts":["background.js"],
        "persistent": false
    },
    "permissions":[
        "clipboardWrite",
        "clipboardRead",
        "contextMenus",
        "background",
        "tabs"
    ]
}